prefix=/workspaces/Computervision/.conda
exec_prefix=/workspaces/Computervision/.conda
libdir=/workspaces/Computervision/.conda/lib
includedir=/workspaces/Computervision/.conda/include

Name: liblzma
Description: General purpose data compression library
URL: https://xz.tukaani.org/xz-utils/
Version: 5.4.6
Cflags: -I${includedir}
Cflags.private: -DLZMA_API_STATIC
Libs: -L${libdir} -llzma
Libs.private: -pthread -lpthread
